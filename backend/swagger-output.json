          },
          "metadata": {
            "type": "object",
            "properties": {}
          }
        },
        "required": [
          "status",
          "message",
          "metadata"
        ]
      },
      "forgotPasswordResponse400Schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "number",
            "example": 400
          },
          "message": {
            "type": "string",
            "example": "No account with the supplied email exists!"
          }
        },
        "required": [
          "status",
          "message"
        ]
      },
      "forgotPasswordResponse424Schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "number",
            "example": 424
          },
          "message": {
            "type": "string",
            "example": "Password reset email failed!"
          }
        },
        "required": [
          "status",
          "message"
        ]
      },
      "resetPasswordBodySchema": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string",
            "example": "string"
          },
          "passwordConfirm": {
            "type": "string",
            "example": "string"
          }
        },
        "required": [
          "password",
          "passwordConfirm"
        ]
      },
      "resetPasswordResponse200Schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "number",
            "example": 200
          },
          "message": {
            "type": "string",
            "example": "Reset password successfully!"
          },
          "metadata": {
            "type": "object",
            "properties": {
              "tokens": {
                "type": "object",
                "properties": {
                  "accessToken": {
                    "type": "string",
                    "example": "string"
                  },
                  "refreshToken": {
                    "type": "string",
                    "example": "string"
                  }
                },
                "required": [
                  "accessToken",
                  "refreshToken"
                ]
              },
              "user": {
                "type": "object",
                "properties": {
                  "_id": {
                    "type": "string",
                    "example": "string"
                  },
                  "name": {
                    "type": "string",
                    "example": "string"
                  },
                  "email": {
                    "type": "string",
                    "example": "string"
                  },
                  "role": {
                    "type": "string",
                    "example": "string"
                  },
                  "typeOfAccount": {
                    "type": "string",
                    "example": "string"
                  },
                  "dateOfBirth": {
                    "type": "string",
                    "example": "string"
                  },
                  "gender": {
                    "type": "string",
                    "example": "string"
                  }
                },
                "required": [
                  "_id",
                  "name",
                  "email",
                  "role",
                  "typeOfAccount",
                  "dateOfBirth",
                  "gender"
                ]
              }
            },
            "required": [
              "tokens",
              "user"
            ]
          }
        },
        "required": [
          "status",
          "message",
          "metadata"
        ]
      },
      "resetPasswordResponse400Schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "number",
            "example": 400
          },
